15 07 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$INPUT_MUZO-$OD_ORG_CIZP-$OD_FORM_ZAVAZEK.conf -l $LOGPATH/$OD_ORG_CIZP/$OD_FORM_ZAVAZEK


# OpenData export
05 08 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_AOPK-$OD_SET_FAKTURY.conf -l $LOGPATH/$OD_ORG_AOPK/$OD_SET_FAKTURY
15 08 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_AOPK-$OD_SET_OBJEDNAVKY.conf -l $LOGPATH/$OD_ORG_AOPK/$OD_SET_OBJEDNAVKY
15 08 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_AOPK-$OD_SET_SMLOUVY.conf -l $LOGPATH/$OD_ORG_AOPK/$OD_SET_SMLOUVY
35 08 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_MZP-$OD_SET_FAKTURY.conf -l $LOGPATH/$OD_ORG_MZP/$OD_SET_FAKTURY
45 08 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_MZP-$OD_SET_OBJEDNAVKY.conf -l $LOGPATH/$OD_ORG_MZP/$OD_SET_OBJEDNAVKY
55 08 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_SJCR-$OD_SET_FAKTURY.conf -l $LOGPATH/$OD_ORG_SJCR/$OD_SET_FAKTURY
05 09 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_SJCR-$OD_SET_SMLOUVY.conf -l $LOGPATH/$OD_ORG_SJCR/$OD_SET_SMLOUVY
15 09 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_CGS-$OD_SET_FAKTURY.conf -l $LOGPATH/$OD_ORG_CGS/$OD_SET_FAKTURY
25 09 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_CGS-$OD_SET_OBJEDNAVKY.conf -l $LOGPATH/$OD_ORG_CGS/$OD_SET_OBJEDNAVKY
35 09 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_CGS-$OD_SET_SMLOUVY.conf -l $LOGPATH/$OD_ORG_CGS/$OD_SET_SMLOUVY
45 09 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_CENIA-$OD_SET_FAKTURY.conf -l $LOGPATH/$OD_ORG_CENIA/$OD_SET_FAKTURY
55 09 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_CIZP-$OD_SET_FAKTURY.conf -l $LOGPATH/$OD_ORG_CIZP/$OD_SET_FAKTURY
05 10 * * $RUNDAY_ALL $RUNUSER $LOGSTASH -f $CONFPATH/$OUTPUT_CSV-$OD_ORG_CIZP-$OD_SET_OBJEDNAVKY.conf -l $LOGPATH/$OD_ORG_CIZP/$OD_SET_OBJEDNAVKY
